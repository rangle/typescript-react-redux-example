{
  "name": "typescript-react-redux-starter",
  "version": "0.1.0",
  "description": "A boiler-plate setup for TypeScript/React/Redux applications",
  "license": "MIT",
  "engines": {
    "node": "4.x"
  },
  "scripts": {
    "postinstall": "npm run clean; npm run typings && npm run build",
    "dev": "cross-env NODE_ENV=development node server.js",
    "start": "cross-env NODE_ENV=production node server.js",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "build": "cross-env NODE_ENV=production webpack",
    "typings": "rimraf typings && typings install",
    "test": "npm run lint && ts-node ./node_modules/mocha/bin/_mocha --opts ./test/mocha.opts",
    "test:watch": "ts-node ./node_modules/mocha/bin/_mocha --opts ./test/mocha.opts --watch",
    "lint": "tslint 'src/**/*.tsx' 'src/**/*.ts' 'test/**/*.tsx' 'test/**/*.ts'"
  },
  "keywords": [
    "react",
    "redux",
    "typescript",
    "starter",
    "seed"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/rangle/typescript-react-redux-starter.git"
  },
  "bugs": {
    "url": "https://github.com/rangle/rangle-starter/issues"
  },
  "devDependencies": {
    "basscss": "^7.1.0",
    "basscss-radium": "^3.0.0",
    "chalk": "^1.1.1",
    "cross-env": "^1.0.7",
    "css-loader": "^0.23.1",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^0.9.0",
    "file-loader": "^0.8.4",
    "history": "^1.17.0",
    "html-webpack-plugin": "^1.6.2",
    "http-proxy": "^1.12.0",
    "immutable": "^3.7.6",
    "istanbul": "^1.0.0-alpha.2",
    "jsdom": "^8.0.2",
    "mocha": "^2.4.5",
    "object-assign": "^4.0.1",
    "radium": "^0.16.2",
    "raw-loader": "^0.5.1",
    "react": "^0.14.6",
    "react-dom": "^0.14.6",
    "react-redux": "^4.0.6",
    "react-router": "^2.0.0-rc5",
    "react-router-redux": "^2.1.0",
    "redux": "^3.0.4",
    "redux-devtools": "^3.0.1",
    "redux-devtools-dock-monitor": "^1.0.1",
    "redux-devtools-log-monitor": "^1.0.1",
    "redux-form": "^4.1.1",
    "redux-localstorage": "^0.4.0",
    "redux-logger": "^2.1.4",
    "redux-thunk": "^1.0.3",
    "require-hacker": "^2.1.3",
    "rimraf": "^2.4.3",
    "serve-webpack-client": "0.0.4",
    "style-loader": "^0.13.0",
    "ts-loader": "^0.8.0",
    "ts-node": "^0.5.5",
    "tslint": "^3.4.0",
    "tslint-loader": "^2.1.0",
    "typescript": "^1.8.0",
    "typings": "^0.6.5",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.9",
    "webpack-hot-middleware": "^2.6.4",
    "winston": "^2.1.1"
  }
}
